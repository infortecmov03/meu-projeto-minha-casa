{
  "name": "structuralcad-pro-universal",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run serve-backend\" \"npm run serve-frontend\"",
    "serve-backend": "node src/server/server.js",
    "serve-frontend": "vite",
    "build": "vite build",
    "build-electron": "electron-builder",
    "package": "npm run build && npm run build-electron",
    "test": "jest",
    "test:importers": "jest src/importers/",
    "test:simulation": "jest src/construction-sim/",
    "benchmark": "node benchmarks/performance.js"
  },
  "dependencies": {
    
    "three": "^0.158.0",
    "cannon-es": "^0.20.0",
    "ammojs-typed": "^1.0.6",
    
    
    "web-ifc": "^0.0.53",
    "fflate": "^0.8.0",
    "draco3d": "^1.5.6",
    "basis-transcoder": "^1.16.4",
    "ktx-parse": "^0.4.0",
    
    
    "dwg": "^0.3.0",
    "dxf-parser": "^1.0.0",
    "obj-file-parser": "^0.5.0",
    "stl": "^1.2.0",
    "collada-parser": "^0.1.0",
    
    
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mui/material": "^5.14.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    
    
    "ffmpeg.wasm": "^0.12.2",
    "recordrtc": "^5.6.2",
    "mediarecorder": "^1.2.0",
    
   
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "axios": "^1.5.0",
    "socket.io-client": "^4.6.1",
    
  
    "sqlite3": "^5.1.6",
    "sequelize": "^6.32.0",
    
  
    "rapier3d": "^0.11.0"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "vite": "^4.4.0",
    "@vitejs/plugin-react": "^4.0.0",
    "typescript": "^5.1.0",
    "jest": "^29.6.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "webpack": "^5.88.0"
  },
  "build": {
    "appId": "com.structuralcad.pro",
    "productName": "StructuralCAD Pro",
    "files": [
      "dist/**/*",
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icons/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}